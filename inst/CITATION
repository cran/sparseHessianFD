year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{sparseHessianFD}:An R Package for Estimating Sparse Hessians",
         author = person(given="Michael", family="Braun"),
         year = year,
         note = note,
         url = "http://CRAN.R-project.org/package=sparseHessianFD")
